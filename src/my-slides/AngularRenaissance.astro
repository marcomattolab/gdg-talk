---
import Slide from "../components/Slide.astro";
import Strong from "../components/Strong.astro";

type Props = {
  n: string;
};

const { n } = Astro.props;
---

<Slide n={n}>
  <h class="text-8xl text-center font-black tracking-tighter">
    Angular <Strong>Renaissance</Strong>
  </h>
  <br>
  <img src="/imgs/angular-renaissance.png" alt="Renaissance Angular" width="100%">
</Slide>
